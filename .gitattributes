# Autodetect text files
* text=auto

*.isml text

# Force all text file types to have unix line endings
*.* text eol=lf

core.autocrlf=input

# Force the following file types to be handled as binaries
*.jpg binary
*.jpeg binary
*.gif binary
*.png binary
*.ttf binary
*.eof binary
*.eot binary
*.swf binary
*.mov binary
*.mp4 binary
*.mp3 binary
*.ogg binary
*.flv binary
*.wav binary
*.jar binary
*.woff2 binary
*.woff binary
